{
  "root": true,
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "commonjs"
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-undef": 0,
    "no-unused-vars": 1
  },
  "globals": {
    "process": true
  }
}
